{
    "name": "firith/laravel-user-status",
    "description": "This is my package laravel-user-status",
    "keywords": [
        "firith",
        "laravel",
        "laravel-user-status"
    ],
    "homepage": "https://github.com/firith/laravel-user-status",
    "license": "MIT",
    "authors": [
        {
            "name": "Bal√°zs Simon",
            "email": "42.simon@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10",
        "orchestra/testbench": "^6.22",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Firith\\LaravelUserStatus\\": "src",
            "Firith\\LaravelUserStatus\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Firith\\LaravelUserStatus\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Firith\\LaravelUserStatus\\LaravelUserStatusServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
